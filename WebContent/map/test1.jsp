<%@ page contentType="text/html;charset=utf-8" %>
<!DOCTYPE html>
<html>
<body>

<h1>My First Google Map</h1>

<div id="googleMap" style="width:100%;height:600px;"></div>

<script>

function myMap() { 
	var latLng=new google.maps.LatLng(37.571064, 126.992336);
	
	var mapProp= {
	  center:latLng,
	  zoom:18,
	};
	var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);
	
	var marker = new google.maps.Marker({
		position: latLng, 
		animation:google.maps.Animation.BOUNCE,
		icon:"https://cdn3.iconfinder.com/data/icons/tango-icon-library/48/face-smile-big-128.png"
	});
	marker.setMap(map);//맵에 마커 등록!!!


		new google.maps.Marker({
			position: new google.maps.LatLng(37.572064, 126.992336), 
			animation:google.maps.Animation.BOUNCE,
			icon:"https://cdn3.iconfinder.com/data/icons/tango-icon-library/48/face-smile-big-128.png"
		}).setMap(map);
		
		new google.maps.Marker({
			position: new google.maps.LatLng(37.573064, 126.992336), 
			animation:google.maps.Animation.BOUNCE,
			icon:"https://cdn3.iconfinder.com/data/icons/tango-icon-library/48/face-smile-big-128.png"
		}).setMap(map);
		
		new google.maps.Marker({
			position: new google.maps.LatLng(37.574064, 126.992336), 
			animation:google.maps.Animation.BOUNCE,
			icon:"https://cdn3.iconfinder.com/data/icons/tango-icon-library/48/face-smile-big-128.png"
		}).setMap(map);


	var infowindow = new google.maps.InfoWindow({
		  content:"이곳이 <h3 style='color:red'><img src='data:image/jpeg;base64,'></h3>이다"
	});

	google.maps.event.addListener(marker,'click',function() {
	  	//map.setZoom(9);
	  	map.setCenter(marker.getPosition());
		infowindow.open(map,marker);
		var pos = map.getZoom();//기존에 설정된 줌의 중심점!!
		map.setZoom(15);
		
		//일정 시간 뒤에 무언가 하고 싶을때...
		window.setTimeout(function() {map.setZoom(pos);},2400);
	});
	
}
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjVjeC63IVSLbDdl7ALW8vpGjC5b55QJc&callback=myMap"></script>

</body>
</html>




